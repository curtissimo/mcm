{{> top}}
<div class="member-page">
  <div class="menu">
  {{{menu}}}
  </div>
  <div class="content">
    <div class="pure-g">
      {{#each members}}
        <div class="member pure-u-sm-1-2 pure-u-md-1-4" data-url="{{memberUrl}}">
          {{#if photoPath}}
            <div class="member-photo">
              <img src="{{photoPath}}" width="100" height="100">
            </div>
          {{else}}
            <div class="member-photo">
              <img src="{{../../unknownMemberPath}}" width="100" height="100">
            </div>
          {{/if}}
          <div class="member-lastName">
            {{lastName}}
          </div>
          <div class="member-firstName">
            {{firstName}}
          </div>
        </div>
      {{/each}}
    </div>
  </div>
  <div class="footer">
  {{{footer}}}
  </div>
</div>
{{> bottom}}
