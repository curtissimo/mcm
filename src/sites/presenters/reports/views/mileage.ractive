<section>
  <table class="pure-table pure-table-striped">
    <thead>
      <tr>
        <th>Year</th>
        <th>Month</th>
        <th>Miles</th>
        <th>Member</th>
      </tr>
    </thead>
    <tbody>
      {{#entries}}
      <tr>
        <td>{{ year }}</td>
        <td>{{ month }}</td>
        <td>{{ miles }}</td>
        <td>{{ who.lastName }}, {{ who.firstName }}</td>
      </tr>
      {{/entries}}
    </tbody>
  </table>
</section>